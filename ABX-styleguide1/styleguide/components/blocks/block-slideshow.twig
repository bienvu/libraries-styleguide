{% block blockSlideshow %}
<div class="block-slideshow">
  <div class="banner-caption">
    <h2 class="banner-caption__title">VETERINARY</h2>
    <div class="banner-caption__description">
      <p>Combining the best in on-site diagnostic with expert reference laboratory services.</p>
    </div>
  </div>
  <div class="block-slideshow__slide js-block-slideshow__slide slick-dots-white">
    {% for item in slideshow %}
      <div>
        {% set block_banner = [ item ] %}
        {% include "block-banner.twig" with block_banner %}
      </div>
    {% endfor %}
  </div>
</div>
{% endblock %}
