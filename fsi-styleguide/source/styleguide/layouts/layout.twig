<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="shortcut icon" href="../favicon.ico">
    <title>{{ site_title }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block head %}
      <!-- <link rel="stylesheet" href="css/style.css"> -->
      <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">
      <link rel="stylesheet" href="/bootstrap/css/bootstrap-responsive.min.css">
      <link rel="stylesheet" href="/fontawesome/css/font-awesome.min.css">
      <link rel="stylesheet" href="../css/styles.css">
      <script src="script/jquery.min.js"></script>
      <script src="script/drupal/jquery.once.js"></script>
      <script src="script/drupal/drupal.js"></script>
      <script src="../js/lib/picturefill.js"></script>
      <script src="../js/lib/froogaloop2.min.js"></script>
      <script src="../js/lib/iframe_api.js"></script>
    {% endblock %}
  </head>
  {% if bodyClass is not empty %}
    <body class="{{ bodyClass }}">
  {% else %}
    <body>
  {% endif %}
    <div class="page-wrapper">
      {% block header %}
        {% include 'box-nav.twig' %}
        {% include 'navigation.twig' %}
      {% endblock %}
      {% block content %}{% endblock %}
      {% block footer %}
        {% include 'footer.twig' %}
      {% endblock %}
    </div>
    {% block footer_scripts %}
      <script src="../js/lib/modernizr.min.js"></script>
      <script src="../js/lib/media.match.min.js"></script>
      <script src="../js/lib/enquire.min.js"></script>
      <script src="../js/lib/slick.min.js"></script>
      <script src="../js/lib/waypoints.min.js"></script>
      <script src="../js/lib/jquery.counterup.js"></script>
      <script src="../js/lib/jquery.matchHeight.js"></script>
      <script src="../js/lib/jquery.equalheights.js"></script>
      <script src="../js/scripts.js"></script>
    {% endblock %}
  </body>
</html>
