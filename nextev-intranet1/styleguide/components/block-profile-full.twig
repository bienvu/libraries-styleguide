{% for item in block_profile_full %}
  {% if item.title is not empty %}
    <div class="block-profile-full {{ item.class }}">
      <div class="block-profile-full__wrap">
        <h2 class="block-profile-full__title">{{ item.title }}</h2>
        {% if item.description is not empty %}
          <div class="block-profile-full__description">{{ item.description }}</div>
        {% endif %}
        {% if item.image is not empty %}
          <div class="block-profile-full__image"><img src="../images-demo/500x500/{{ item.image }}" alt="image-profile-full"></div>
        {% endif %}
        {% if item.link is not empty or item.link1 is not empty %}
          <div class="block-profile-full__link">
            {{ item.link }}
            {{ item.link1 }}
          </div>
        {% endif %}
      </div>
    </div>
  {% endif %}
{% endfor %}
