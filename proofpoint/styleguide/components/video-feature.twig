{% block videoFeature %}
<h3>Video Feature Upload</h3>
<div class="video-feature">
  <div class="video-feature__image">
    <picture>
      <source
        media="{{ desktop }}"
        srcset="https://unsplash.it/1366/768 1x">
      <source
        media="{{ tablet }}"
        srcset="https://unsplash.it/1024/576 1x">
      <source
        media="{{ mobile_landscape }}"
        srcset="https://unsplash.it/768/380 1x">
      <source
        media="{{ mobile }}"
        srcset="https://unsplash.it/480/380 1x">
      <img
        src="https://unsplash.it/480/380"
        alt="ppoint image">
    </picture>
  </div>
  <div class="video-feature__video">
    <video width="1280" height="720" muted="true" loop="loop" autoplay="true" preload="auto">
      <source src="{{ video_path }}video-feature.mp4" type="video/mp4">
    </video>
  </div>
  <div class="video-feature__content">
    <div class="container">
      <h1 class="video-feature__title">Nulla et lacus vitae dui faucibus posuere vel in est</h1>
      <a href="#" class="btn btn--outline-white">Read more</a>
    </div>
  </div>
</div>
