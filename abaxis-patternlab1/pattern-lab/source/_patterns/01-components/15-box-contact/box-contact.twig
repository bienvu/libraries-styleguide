<div class="block-contact-info__wrap margin-bottom-large">
  {% if boxContact.title is not empty %}
    <h3 class="block-contact-info__title">{{boxContact.title}}</h3>
  {% endif %}
  {% for item in boxContact.items %}
    {{item}}
  {% endfor %}
  {% if boxContact.link is not empty %}
  <div class="block-contact-info__link">{{boxContact.link}}</div>
  {% endif %}
</div>
