{% if site_logo %}
  <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home" class="logo">
    <img src="{{ site_logo }}" alt="{{ 'Home'|t }}" class="hidden-on-desktopOnly" />
    {% if site_logo_mobile %}
      <img src="{{ site_logo_mobile }}" alt="{{ 'Home'|t }}" class="hidden-on-desktop" />
    {% endif %}
  </a>
{% endif %}
