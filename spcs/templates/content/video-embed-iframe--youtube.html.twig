{#
/**
 * @file
 * Display an iframe with alterable components.
 */
#}
{% set query = query|merge({'enablejsapi': 1}) %}

<iframe{{ attributes.addClass('youtube-embed') }}{% if url is not empty %} src="{{ url }}{% if query is not empty %}?{{ query | url_encode }}{% endif %}{% if fragment is not empty %}#{{ fragment }}{% endif %}"{% endif %}></iframe>
