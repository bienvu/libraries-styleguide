{% set items = [] %}
{% set count  = content.field_icon_text_sidebar_item['#items'].getValue() | length %}

{% for i in 0..count if i < count %}
  {% set items = items|merge([content.field_icon_text_sidebar_item[i]]) %}
{% endfor %}

{%
  include '@components/27-contact/_box-contact-info.twig' with {
    'contactInfo': {
      'title': content.field_title.0,
      'items': items,
    }
  } only
%}
