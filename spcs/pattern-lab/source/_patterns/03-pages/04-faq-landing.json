{
  "pageclass" : "",
  "mainRegion": {
    "include()": {
      "pattern": "templates-content-layout",
      "with": {
        "contentLayout": {
          "items": [
            {
              "include()": {
                "pattern": "components-navigation-block"
              }
            },
            {
              "include()": {
                "pattern": "components-faq-section"
              }
            },
            {
              "include()": {
                "pattern": "components-cta",
                "with": {
                  "cta": {
                    "bgColor": "bg--accent text--white",
                    "title": "Join our mailing list",
                    "description": "Nam porttitor blandit accumsan. Ut vel dictum sem, a pretium dui.",
                    "link": {
                      "include()": {
                        "pattern": "base-elements-link",
                        "with": {
                          "link": {
                            "class": "link--bold",
                            "href": "#",
                            "text": "Learn more"
                          },
                          "only": true
                        }
                      }
                    }
                  }
                },
                "only": true
              }
            },
            {
              "include()": {
                "pattern": "components-page-follow"
              }
            },
            {
              "include()": {
                "pattern": "components-footer-bar"
              }
            }
          ]
        }
      },
      "only": true
    }
  },
  "footerContent": {
    "include()": {
      "pattern": "components-footer"
    }
  },
  "headerContent": {
    "include()": {
      "pattern": "components-header"
    }
  }
}
